# Configure the JMS test queue
/subsystem=messaging-activemq/server=default/jms-queue=testQueue:add(entries=["queue/test","java:jboss/exported/jms/queue/test"])

/subsystem=messaging-activemq/server=default/pooled-connection-factory=NewRemoteConnectionFactory:add(entries=["java:/NewRemoteConnectionFactory", "java:jboss/exported/jms/NewRemoteConnectionFactory"],connectors=["http-connector"])

/subsystem=messaging-activemq/server=default:write-attribute(name=security-enabled, value=false)
